---
import Layout from '../layouts/Layout.astro';
const files =await Astro.glob('./zh-cn/**/*.md')
---

<Layout title="正式操作">
	<main class="flex flex-col gap-2">

        {files.map(i=>{
            return <a href={i.url}>{
                i.frontmatter.title
            }</a>
        })}
	</main>
</Layout>
