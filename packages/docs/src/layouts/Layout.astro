---
import BaseHead from '../components/BaseHead.astro'
import { Header } from '../components/Header'
interface Props {
    title: string
    description: string
}
const { title, description } = Astro.props
---

<html lang="en">
    <head>
        <BaseHead title={title} description={description} />
    </head>

    <body>
        <Header client:load />
        <slot />
    </body>
</html>
